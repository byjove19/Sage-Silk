    <style>
        /* Sage & Silk Kids Collection Custom Styles */
        :root {
            --sage-primary: #9ca986;
            --sage-dark: #6b7460;
            --sage-light: #a8b894;
            --silk-cream: #f5f3f0;
            --silk-warm: #e8e4df;
            --silk-beige: #d4c5b0;
            --text-dark: #4a5240;
            --text-light: #7a8268;
            --pastel-pink: #f8d7d7;
            --pastel-blue: #d7e3f8;
            --pastel-green: #d7f8e2;
            --pastel-yellow: #f8f0d7;
        }

        /* Kids Collection Hero */
        .kids-hero {
            background: linear-gradient(135deg, rgba(248, 215, 215, 0.2) 0%, rgba(215, 227, 248, 0.2) 50%, rgba(215, 248, 226, 0.2) 100%);
            padding: 80px 0;
            position: relative;
            overflow: hidden;
        }

        .kids-hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-image: url('/assets2/images/kids/kids-pattern.png');
            opacity: 0.05;
            z-index: 0;
        }

        .kids-hero-content {
            position: relative;
            z-index: 1;
        }

        .kids-hero h1 {
            font-family: 'Comic Sans MS', cursive, sans-serif;
            color: var(--sage-dark);
            font-size: 3.5rem;
            font-weight: 700;
            text-shadow: 2px 2px 4px rgba(0,0,0,0.1);
        }

        .kids-hero p {
            font-size: 1.2rem;
            color: var(--text-dark);
            max-width: 600px;
            margin: 0 auto 30px;
        }

        .kids-hero .btn {
            background-color: var(--pastel-pink);
            color: var(--sage-dark);
            border: none;
            padding: 12px 30px;
            font-weight: 600;
            border-radius: 30px;
            box-shadow: 0 4px 15px rgba(156, 169, 134, 0.2);
            transition: all 0.3s ease;
        }

        .kids-hero .btn:hover {
            background-color: var(--sage-primary);
            color: white;
            transform: translateY(-3px);
            box-shadow: 0 8px 25px rgba(156, 169, 134, 0.3);
        }

        /* Age Group Cards */
        .age-group-card {
            border-radius: 20px;
            overflow: hidden;
            box-shadow: 0 10px 30px rgba(0,0,0,0.05);
            transition: all 0.3s ease;
            border: 2px solid white;
            position: relative;
        }

        .age-group-card:hover {
            transform: translateY(-10px);
            box-shadow: 0 15px 40px rgba(0,0,0,0.1);
            border-color: var(--sage-primary);
        }

        .age-group-card img {
            transition: transform 0.5s ease;
        }

        .age-group-card:hover img {
            transform: scale(1.05);
        }

        .age-group-card h3 {
            position: absolute;
            bottom: 20px;
            left: 0;
            right: 0;
            background: rgba(255,255,255,0.8);
            padding: 10px;
            margin: 0 20px;
            border-radius: 10px;
            text-align: center;
            color: var(--sage-dark);
            font-weight: 700;
        }

        /* Product Cards */
        .kids-product-card {
            background: white;
            border-radius: 15px;
            overflow: hidden;
            box-shadow: 0 5px 15px rgba(0,0,0,0.05);
            transition: all 0.3s ease;
            border: 1px solid var(--silk-warm);
            position: relative;
        }

        .kids-product-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 25px rgba(0,0,0,0.1);
        }

        .kids-product-card .product-badge {
            position: absolute;
            top: 10px;
            right: 10px;
            background-color: var(--pastel-pink);
            color: var(--sage-dark);
            padding: 5px 10px;
            border-radius: 20px;
            font-size: 0.8rem;
            font-weight: 600;
        }

        .kids-product-card .product-image {
            height: 250px;
            overflow: hidden;
            display: flex;
            align-items: center;
            justify-content: center;
            background-color: var(--silk-cream);
        }

        .kids-product-card .product-image img {
            max-height: 100%;
            transition: transform 0.5s ease;
        }

        .kids-product-card:hover .product-image img {
            transform: scale(1.05);
        }

        .kids-product-card .product-info {
            padding: 15px;
        }

        .kids-product-card .product-title {
            font-size: 1rem;
            color: var(--sage-dark);
            font-weight: 600;
            margin-bottom: 5px;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }

        .kids-product-card .product-price {
            font-weight: 700;
            color: var(--sage-primary);
        }

        .kids-product-card .product-price .old-price {
            text-decoration: line-through;
            color: var(--text-light);
            font-size: 0.9rem;
            margin-right: 5px;
        }

        /* Category Tabs */
        .kids-category-tabs .nav-tabs {
            border-bottom: 2px solid var(--silk-warm);
            justify-content: center;
        }

        .kids-category-tabs .nav-link {
            border: none;
            color: var(--text-light);
            font-weight: 600;
            padding: 10px 25px;
            margin: 0 5px;
            border-radius: 30px;
            transition: all 0.3s ease;
        }

        .kids-category-tabs .nav-link:hover {
            color: var(--sage-dark);
            background-color: var(--silk-cream);
        }

        .kids-category-tabs .nav-link.active {
            background-color: var(--sage-primary);
            color: white;
        }

        /* Featured Collection */
        .featured-collection {
            background-color: var(--pastel-blue);
            padding: 60px 0;
            position: relative;
            overflow: hidden;
        }

        .featured-collection::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-image: url('/assets2/images/kids/dots-pattern.png');
            opacity: 0.1;
            z-index: 0;
        }

        .featured-collection-content {
            position: relative;
            z-index: 1;
        }

        /* Responsive Adjustments */
        @media (max-width: 768px) {
            .kids-hero h1 {
                font-size: 2.5rem;
            }
            
            .kids-hero p {
                font-size: 1rem;
            }
            
            .age-group-card h3 {
                font-size: 1rem;
                bottom: 10px;
                margin: 0 10px;
            }
        }
    </style>
</head>

